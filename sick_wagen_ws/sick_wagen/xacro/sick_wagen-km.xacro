<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="whill_modelc">

  <material name="gray">
    <color rgba="0.5 0.5 0.5 1.0" />
  </material>
  <material name="red">
    <color rgba="1.0 0.0 0.0 1.0" />
  </material>
  <material name="green">
    <color rgba="0.0 1.0 0.0 1.0" />
  </material>
  <material name="blue">
    <color rgba="0.0 0.0 1.0 1.0" />
  </material>
  <material name="black">
    <color rgba="0.2 0.2 0.2 1.0" />
  </material>

  <link name="base_link" />
  <joint name="base_body_joint" type="fixed">
    <parent link="base_link" />
    <child link="base_floor" />
    <origin xyz="0.3 0 0.0" rpy="0 0 0" />
  </joint>

  <link name="base_floor">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.85 0.4 0.050" />
      </geometry>
      <material name="green" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.85 0.4 0.050" />
      </geometry>
    </collision>
  </link>

  <link name="left_wheel_link">
    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1325" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <material name="red" />
    </visual>

    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1325" />
      </geometry>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.500" />
      <inertia ixx="0.0013541667" ixy="0" ixz="0" iyy="0.0013541667" iyz="0" izz="0.0025" />
    </inertial>
  </link>


  <joint name="leftWheel" type="continuous">
    <parent link="base_floor" />
    <child link="left_wheel_link" />
    <origin xyz="-0.3 0.245 0" rpy="-1.57 0 0" />
    <axis xyz="0 0 1" />
  </joint>


  <link name="right_wheel_link">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.05" radius="0.1325" />
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder length="0.05" radius="0.1325" />
      </geometry>
      <material name="red" />
    </visual>
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.500" />
      <inertia ixx="0.0013541667" ixy="0" ixz="0" iyy="0.0013541667" iyz="0" izz="0.0025" />
    </inertial>
  </link>

  <joint name="rightWheel" type="continuous">
    <parent link="base_floor" />
    <child link="right_wheel_link" />
    <origin xyz="-0.3 -0.245 0" rpy="1.57 0 0" />
    <axis xyz="0 0 1" />
  </joint>


  <link name="frontwheel_left">
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1325" />
      </geometry>
    </collision>

    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1325" />
      </geometry>
      <material name="red" />
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0" />
    </inertial>
  </link>

  <joint name="frontwheel_left_joint" type="fixed">
    <parent link="base_floor" />
    <child link="frontwheel_left" />
    <origin xyz="0.3 -0.245 0" rpy="1.57 0 0" />
    <axis xyz="0 0 1" />
  </joint>
  <link name="frontwheel_right">
    <collision>
      <geometry>
        <cylinder length="0.05" radius="0.1325" />
      </geometry>
    </collision>

    <visual>
      <geometry>
        <cylinder length="0.05" radius="0.1325" />
      </geometry>
      <material name="red" />
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0" />
    </inertial>
  </link>

  <joint name="frontwheel_right_joint" type="fixed">
    <parent link="base_floor" />
    <child link="frontwheel_right" />
    <origin xyz="0.3 0.245 0" rpy="1.57 0 0" />
    <axis xyz="0 0 -1" />
  </joint>

  <!-- tim_frame-->
  <joint name="tim_frame_joint" type="fixed">
    <parent link="base_link" />
    <child link="tim_frame" />
    <origin xyz="0.82 0 0.0" rpy="0 0 0" />
  </joint>
  <link name="tim_frame">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.125 0.66 0.050" />
      </geometry>
      <material name="black" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.10 0.66 0.050" />
      </geometry>
    </collision>
  </link>
  <!-- Tim_R %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->
  <joint name="tim_joint_R" type="fixed">
    <parent link="base_link" />
    <child link="tim_link_R" />
    <origin xyz="0.82 -0.29 0.075" rpy="0 0 -0.84806208" />
  </joint>
  <link name="tim_link_R">
    <visual>
      <geometry>
        <cylinder radius="0.030" length="0.086" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <material name="blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.030" length="0.086" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.25" />
      <inertia ixx="0.00021033333"
        ixy="0" ixz="0"
        iyx="0" iyy="0.00021033333"
        iyz="0"
        izx="0" izy="0" izz="0.0001125" />
    </inertial>
  </link>

  <!-- Tim_L %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->
  <joint name="tim_joint_L" type="fixed">
    <parent link="base_link" />
    <child link="tim_link_L" />
    <origin xyz="0.82 0.29 0.075" rpy="0 0 0.84806208" /> <!--ベースリンクからの距離 -->
  </joint>
  <link name="tim_link_L">
    <visual>
      <geometry>
        <cylinder radius="0.030" length="0.086" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <material name="blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.030" length="0.086" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.25" />
      <inertia ixx="0.00021033333"
        ixy="0" ixz="0"
        iyx="0" iyy="0.00021033333"
        iyz="0"
        izx="0" izy="0" izz="0.0001125" />
    </inertial>
  </link>

  <!-- laser %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->
  <joint name="laser_joint" type="fixed">
    <parent link="base_link" />
    <child link="laser_link" />
    <origin xyz="0.82 0 0.075" rpy="0 0 0" /> <!--ベースリンクからの距離 -->
  </joint>
  <link name="laser_link">
    <visual>
      <geometry>
        <cylinder radius="0.01" length="0.01" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <material name="blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.01" length="0.01" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.25" />
      <inertia ixx="0.00021033333"
        ixy="0" ixz="0"
        iyx="0" iyy="0.00021033333"
        iyz="0"
        izx="0" izy="0" izz="0.0001125" />
    </inertial>
  </link>

  <!-- multiscan %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->
  <joint name="multiscan_joint" type="fixed">
    <parent link="base_link" />
    <child link="multiscan_link" />
    <origin xyz="-0.10 0 1.545" rpy="0 0 0" />
  </joint>
  <link name="multiscan_link">
    <visual>
      <geometry>
        <cylinder radius="0.060" length="0.086" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <material name="blue" />
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.060" length="0.086" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.25" />
      <inertia ixx="0.00021033333"
        ixy="0" ixz="0"
        iyx="0" iyy="0.00021033333"
        iyz="0"
        izx="0" izy="0" izz="0.0001125" />
    </inertial>
  </link>

  <!-- imu %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->
  <joint name="imu_joint" type="fixed">
    <parent link="base_link" />
    <child link="imu_link" />
    <origin xyz="-0.14 0 1.05" rpy="0 0 0" />
  </joint>
  <link name="imu_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.06 0.06 0.020" />
      </geometry>
      <material name="black" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.10 0.66 0.050" />
      </geometry>
    </collision>
  </link>

  <!-- web camear %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->
  <joint name="camera_joint" type="fixed">
    <parent link="base_link" />
    <child link="camera_link" />
    <origin xyz="-0.14 0 1.40" rpy="0 0 0" />
  </joint>
  <link name="camera_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.06 0.06 0.020" />
      </geometry>
      <material name="black" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.10 0.66 0.050" />
      </geometry>
    </collision>
  </link>

  <!-- gps %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%-->
  <joint name="gps_joint" type="fixed">
    <parent link="base_link" />
    <child link="gps_link" />
    <origin xyz="0.12 0.25 1.03" rpy="0 0 0" />
  </joint>
  <link name="gps_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.06 0.06 0.020" />
      </geometry>
      <material name="black" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.10 0.66 0.050" />
      </geometry>
    </collision>
  </link>
</robot>